import operator as op


greeting = None
greeter = -> print(f"So I just wanted to say, {_}")

# None coalescing.
greeter(greeting)
greeter(greeting ?? "Hello world!")

# Shorthand composition.
maplist = map ..> list

# Shorthand lambdas.
hi = 5
print(maplist(-> (_ + 10 + hi + 30), range(10)))
print(maplist(Î» (_ + 10), range(10)))
print(maplist(-> _ + 2, range(2)))

# Currying
a = op.add$(10)(20)
print(a)

add_ten_and_print = ((op.add$(10)) ..> print)
add_ten_and_print(13)
