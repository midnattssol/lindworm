import operator as op

def test_chain(unittester):
    # Normal iterator chain.
    item = range(10, 20) :: range(3, 5)
    item = list(item)

    unittester.assertEqual(item, [*range(10, 20), *range(3, 5)])

    # Chain-equals.
    item = []
    item ::= range(100)
    item = list(item)

    unittester.assertEqual(item, [*range(100)])

    # Chain-star-equals.
    item = range(10)
    item ::*= [range(10), range(10)]
    item = list(item)
    unittester.assertEqual(item, [*range(10), *range(10), *range(10)])
